<div class="row h-auto">
  <%= link_to 'Back', admin_categories_path %>
</div>
<div class="row h-75">
  <%= form_for Teams, as: :teams, url: admin_team_path(@team), html: { method: :put } do |f| %>
    <%= f.label 'Name' %><br>
    <%= f.text_field :name, :value => @team.name %><br>

    <%= f.label 'Location' %><br>
    <%= f.text_field :location, :value => @team.location %><br>

    <%= f.submit "Save" %>
  <% end %>
</div>